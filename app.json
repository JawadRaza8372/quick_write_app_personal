{
	"expo": {
		"name": "QuickWriteAI",
		"slug": "quick-write",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "com.quickwrite",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"bundleIdentifier": "com.quickwrite",
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.quickwrite"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"@stripe/stripe-react-native",
				{
					"enableGooglePay": true
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "e578685c-0462-48b9-81df-f6d1e14816ff"
			}
		}
	}
}
